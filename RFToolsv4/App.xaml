                            <Color x:Key="SystemBaseLowColor">#33333333</Color>
                            <Color x:Key="SystemBaseMediumColor">#19FFFFFF</Color>
                            <Color x:Key="SystemBaseMediumHighColor">#FF2B2B2B</Color>
